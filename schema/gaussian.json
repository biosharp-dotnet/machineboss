{
  "$schema": "http://json-schema.org/schema#",
  "id": "https://raw.githubusercontent.com/ihh/bossmachine/master/schema/gaussian.json",
  "description": "BossMachine Gaussian parameter assignments schema",
  "type": "object",
  "patternProperties": {
    ".*": {
      "type": "object",
      "required": ["mu","tau"],
      "additionalProperties": false,
      "properties": {
	"mu": { "type": "number" },
	"tau": { "type": "number" } }
    }
  }
}
