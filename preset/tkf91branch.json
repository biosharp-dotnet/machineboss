{"state":[
  {"id":"begin",
   "trans":[{"to":"insert","weight":"pDescendants"},
            {"to":"wait","weight":"pNoDescendants"}]},
  {"id":"orphan",
   "trans":[{"to":"insert","weight":"pOrphans"},
            {"to":"wait","weight":"pNoOrphans"}]},
  {"id":"wait",
   "trans":[{"to":"match","weight":"pNoDeletion"},
            {"to":"delete","weight":"pDeletion"},
            {"to":"end"}]},
  {"id":"insert",
   "trans":[{"to":"begin","out":"A","weight":{"/":[1,4]}},
            {"to":"begin","out":"C","weight":{"/":[1,4]}},
            {"to":"begin","out":"G","weight":{"/":[1,4]}},
            {"to":"begin","out":"T","weight":{"/":[1,4]}}]},
  {"id":"match",
   "trans":[{"to":"begin","in":"A","out":"A","weight":"pSame"},
            {"to":"begin","in":"A","out":"C","weight":"pDiff"},
            {"to":"begin","in":"A","out":"G","weight":"pDiff"},
            {"to":"begin","in":"A","out":"T","weight":"pDiff"},
            {"to":"begin","in":"C","out":"A","weight":"pDiff"},
            {"to":"begin","in":"C","out":"C","weight":"pSame"},
            {"to":"begin","in":"C","out":"G","weight":"pDiff"},
            {"to":"begin","in":"C","out":"T","weight":"pDiff"},
            {"to":"begin","in":"G","out":"A","weight":"pDiff"},
            {"to":"begin","in":"G","out":"C","weight":"pDiff"},
            {"to":"begin","in":"G","out":"G","weight":"pSame"},
            {"to":"begin","in":"G","out":"T","weight":"pDiff"},
            {"to":"begin","in":"T","out":"A","weight":"pDiff"},
            {"to":"begin","in":"T","out":"C","weight":"pDiff"},
            {"to":"begin","in":"T","out":"G","weight":"pDiff"},
            {"to":"begin","in":"T","out":"T","weight":"pSame"}]},
  {"id":"delete",
   "trans":[{"to":"orphan","in":"A"},
            {"to":"orphan","in":"C"},
            {"to":"orphan","in":"G"},
            {"to":"orphan","in":"T"}]},
  {"id":"end"}],
 "defs": {
   "pNoDeletion":{"exp":{"*":[-1,{"*":["delRate","time"]}]}},
   "pDeletion":{"not":"pNoDeletion"},
   "pNoInsertion":{"exp":{"*":[-1,{"*":["insRate","time"]}]}},
   "pInsertion":{"not":"pNoInsertion"},
   "delInsRatio":{"/":["pNoDeletion","pNoInsertion"]},
   "pDescendants":{"/":[{"*":["insRate",{"not":"delInsRatio"}]},
                        {"-":["delRate",{"*":["insRate","delInsRatio"]}]}]},
   "pNoDescendants":{"not":"pDescendants"},
   "pNoOrphans":{"*":[{"/":["delRate","insRate"]},
                      {"/":["pDescendants","pDeletion"]}]},
   "pOrphans":{"not":"pNoOrphans"},
   "pNoSub":{"exp":{"*":[-1,"time"]}},
   "pSub":{"not":"pNoSub"},
   "pDiff":{"/":["pSub",4]},
   "pSame":{"+":["pNoSub","pDiff"]}
 },
 "cons":{
   "rate":["time","insRate","delRate"]
 }
}
